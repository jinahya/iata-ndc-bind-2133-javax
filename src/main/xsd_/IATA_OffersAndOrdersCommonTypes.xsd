<xs:documentation>Original Order Item price which was repriced.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="PaymentTimeLimit" type="PaymentTimeLimitType" minOccurs="0" maxOccurs="1">
<xs:annotation>
<xs:documentation>Payment Time Limit</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="PenaltyRefID" type="ID_Type" minOccurs="0" maxOccurs="1">
<xs:annotation>
