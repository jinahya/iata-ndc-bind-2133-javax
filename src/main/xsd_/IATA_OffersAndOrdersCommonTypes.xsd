</xs:annotation>
<xs:sequence>
<xs:element name="AirlineDesigCode" type="AirlineDesigCodeType" minOccurs="1" maxOccurs="1">
<xs:annotation>
<xs:documentation>Airline code assigned to a carrier. Either ICAO-defined 3-character code or IATA-defined 2-character code.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="Arrival" type="TransportPointType" minOccurs="1" maxOccurs="1">
<xs:annotation>
<xs:documentation>This is a transport point location.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="Dep" type="TransportPointType" minOccurs="1" maxOccurs="1">
<xs:annotation>
<xs:documentation>This is a transport point location.</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="FareBasisAppCode" type="CodeType" minOccurs="0" maxOccurs="1">
<xs:annotation>
