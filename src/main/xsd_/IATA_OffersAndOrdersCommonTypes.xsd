<xs:enumeration value="LBR">
<xs:annotation>
<xs:documentation>US Pound</xs:documentation>
</xs:annotation>
</xs:enumeration>
</xs:restriction>
</xs:simpleType>
<xs:simpleType name="WeightUnitCodeType">
<xs:annotation>
<xs:documentation>Specifies Weight Unit code list.</xs:documentation>
</xs:annotation>
<xs:restriction base="WeightUnitCodeContentType"></xs:restriction>
</xs:simpleType>
</xs:schema>
