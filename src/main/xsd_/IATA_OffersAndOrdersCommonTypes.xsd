</xs:annotation>
</xs:element>
<xs:element name="FareRefText" type="TextType" minOccurs="0" maxOccurs="1">
<xs:annotation>
<xs:documentation>Fare reference key, ex: PQ123456</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="FareRuleTextCriteria" type="FareRuleTextCriteriaType" minOccurs="0" maxOccurs="unbounded">
<xs:annotation>
